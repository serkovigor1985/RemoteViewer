<Window x:Class="RV.UserInterface.Forms.ClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ClientWindow" Height="300" Width="300" WindowState="Maximized" WindowStartupLocation="CenterOwner" Closing="ClosingWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Image Grid.Row="0" Name="ImgRemoteDesktop" Stretch="Fill" Grid.Column="0"/>
    </Grid>
</Window>
